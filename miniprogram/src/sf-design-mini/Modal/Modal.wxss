@import '../reset.wxss';

.sf-design-mini__modal {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1100;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
}

.modal__mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0;
}

.modal__operating-area {
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  opacity: 0.6;
  z-index: 1109;
}

.modal__cancel {
  margin-top: 20rpx;
  padding: 30rpx 60rpx 50rpx;
}

.modal__cancel-icon {
  width: 72rpx;
  height: 72rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 72rpx 72rpx;
}